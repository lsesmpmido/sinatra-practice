<form action="/notes" method="post">
  <div class="menu-bar">
    <input type="submit" value="保存" class="btn btn-primary">
    <a href="/notes" class="btn btn-secondary">キャンセル</a>
  </div>
  <div class="sticky-note full">
    <input type="text" name="title" placeholder="ここにタイトルを入力" class="form-control form-title">
    <textarea name="content" placeholder="ここにメッセージを入力" class="form-control form-content"></textarea>
  </div>
</form>
